{
    "version": 2,
    "builds": [
        {
            "src": "./index.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "index.js"
        }
    ],
    "functions": {
        "api/cron-job.js": {
            "memory": 128,
            "maxDuration": 30
        }
    }
}